{
  "version": 2,
  "name": "@yodata/real-estate",
  "alias": [
    "realestate.yodata.me"
  ],
  "builds": [
    {
      "src": "www/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/www/static/$1"
    },
    {
      "src": "/.*",
      "dest": "/www/index.html"
    }
  ]
}
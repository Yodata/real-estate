{
  "markdown.preview.typographer": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.codeActionsOnSave.rules": null,
  "eslint.nodeEnv": "",
  "importCost.bundleSizeDecoration": "minified",
  "importCost.javascriptExtensions": [
    ".js",
    ".jsx",
    ".ts",
    ".tsx"
  ],
  "eslint.format.enable": true,
  "gitHistory.includeRemoteBranches": true,
  "gitProjectManager.displayProjectPath": true,
  "gitProjectManager.ignoredFolders": [
    "node_modules"
  ],
  "markdown.allAvailableLanguages": false,
  "markdown.docsetLanguages": [
    "HTML",
    "HTTP",
    "JavaScript",
    "Bash"
  ],
  "markdown.enableMetadataDateReminder": false,
  "markdown.validate.enabled": true,
  "markdown.validate.referenceLinks.enabled": "error",
  "markdown-preview-enhanced.enableTypographer": true,
  "npm.enableRunFromFolder": true,
  "npm.exclude": "",
  "editor.fontSize": 16,
  "editor.cursorStyle": "block",
  "files.associations": {
    "/template/*.js": "javascriptreact"
  },
  "workbench.editor.autoLockGroups": {
    "vscode.markdown.preview.editor": true
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.defaultLocation": "editor",
  "jest.testExplorer": {
    "enabled": true
  },
  "jest.jestCommandLine": "NODE_OPTIONS=--experimental-vm-modules npx jest",
  "yaml.schemas": {
    "https://raw.githubusercontent.com/awslabs/goformation/master/schema/sam.schema.json": [
      "sam.yaml",

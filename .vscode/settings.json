      "src/schemas/profile/*.yaml",
      "src/schemas/servicearea/*.yaml",
      "src/schemas/team/*.yaml",
      "src/schemas/website/*.yaml"
    ],
    "http://asyncapi.com/definitions/2.5.0/schema.json": [],
    "https://www.asyncapi.com/definitions/2.4.0/asyncapi.json": "file:///Users/dave/git/yodata/real-estate/schemas/asyncapi.yaml"

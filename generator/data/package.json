{
  "name": "@yodata/real-estate-generator-data",
  "description": "generators json data from the realestate.yodata.me/asyncapi.json file",
  "generator": {
    "generator": "1.9.13",
    "renderer": "react",
    "params": {
      "baseUrl": "https://exampe.com/schemas/",
      "outputFormats": {
        "json": {
          "enabled": true,
          "extension": "json",
          "basePath": ""
        },
        "yaml": {
          "enabled": true,
          "extension": "yaml",
          "basePath": ""
        }
      }
    }
  },
  "dependencies": {
    "@asyncapi/generator": "^1.9.13",
    "@asyncapi/generator-filters": "^2.1.0",
    "@asyncapi/generator-react-sdk": "^0.2.25",
    "@asyncapi/parser": "^1.17.1",
    "flatten-anything": "^3.0.2",
    "js-yaml": "^4.1.0",
    "merge-anything": "^5.0.4"
  },
  "scripts": {
    "cleanup": "rimraf ./__generated__",
    "generate": "cleanup && npx ag /Users/dave/git/yodata/real-estate/schemas/_asyncapi.yaml ./ -o ./__generated__ --force-write",
    "generate:production-public-schemas": "npx ag /Users/dave/git/yodata/real-estate/schemas/_asyncapi.yaml ./ -o ../../src/public/schema --force-write"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-react": "^7.31.10"
  },
  "pnpm": {
    "overrides": {
      "ajv@<6.12.3": ">=6.12.3"
    }
  }
}

{
  "version": 2,
  "name": "@yodata/real-estate",
  "builds": [
    {
      "src": "static/*.json",
      "use": "@now/static"
    },
    {
      "src": "www/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/context.json",
      "dest": "/static/context.json"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}